environment 
 tabular_data 
 tabular_data_file = 'dakotaTab.out' 

method,
 sampling
 sample_type = lhs 
 samples = 50 
 seed = 439

variables 
 active uncertain 
  normal_uncertain = 4
    means = 0 0 0 0 
    std_deviations = 1 1 1 1 
    descriptors = 'epsilon_protection_specific_heat' 'epsilon_steel_modulus' 'epsilon_steel_thermal_strain' 'epsilon_steel_thermal_conductivity' 

  lognormal_uncertain = 1
    means = 0.2 
    std_deviations = 0.2 
    descriptors = 'zeta' 

  uniform_uncertain = 3
    lower_bounds = 5 3 2.5 
    upper_bound = 10 8 3.2 
    descriptors = 'room_length1' 'room_length2' 'room_height' 





interface 
  analysis_driver = 'workflow_driver1'
  fork
   parameters_file = 'paramsDakota.in'
   results_file = 'results.out' 
   aprepro 
   work_directory
     named 'workdir' 
     directory_tag
     directory_save
     file_save
     copy_files = 'templatedir/*' 
  asynchronous evaluation_concurrency = 8

responses
 response_functions = 1
 response_descriptors = 'out' 
 no_gradients
 no_hessians

